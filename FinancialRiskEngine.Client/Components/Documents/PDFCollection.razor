
<MudGrid>
    @foreach (var file in FileNames)
    {
        <PDFItem FileName="@file" ContainerName="@ContainerName" />
    }
</MudGrid>


@code {
    [Parameter]
    public string ContainerName { get; set; } = string.Empty;

    [Parameter]
    public List<string> FileNames { get; set; } = new List<string>();
}
